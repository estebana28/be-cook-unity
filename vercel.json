{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "scripts": {
        "build": "prisma generate && nest build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["POST", "GET", "PUT", "PATCH", "DELETE"]
    }
  ]
}
