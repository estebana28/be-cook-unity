generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Pokemon {
  id Int @id @default(autoincrement())
  name String @unique
  attack Int
  type Type
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum Type {
  NORMAL
  FIGHTING
  FLYING
  POISON
  GROUND
  ROCK
  BUG
  GHOST
  STEEL
  FIRE
  WATER
  GRASS
  ELECTRIC
  PSYCHIC
  ICE
  DRAGON
  DARK
  FAIRY
}